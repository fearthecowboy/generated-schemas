{
  "id": "https://schema.management.azure.com/schemas/2016-05-01/Microsoft.InfrastructureInsights.Admin.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Microsoft.InfrastructureInsights.Admin",
  "description": "Microsoft InfrastructureInsights Admin Resource Types",
  "resourceDefinitions": {
    "regionHealths_alerts": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Name of the alert."
        },
        "type": {
          "type": "string",
          "enum": [
            "Microsoft.InfrastructureInsights.Admin/regionHealths/alerts"
          ]
        },
        "apiVersion": {
          "type": "string",
          "enum": [
            "2016-05-01"
          ]
        },
        "tags": {
          "oneOf": [
            {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Resource tags."
        },
        "location": {
          "type": "string",
          "description": "The Azure Region where the resource lives"
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/AlertModel"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Contains alert data."
        }
      },
      "required": [
        "name",
        "type",
        "apiVersion",
        "properties"
      ],
      "description": "Microsoft.InfrastructureInsights.Admin/regionHealths/alerts"
    }
  },
  "definitions": {
    "AlertModel": {
      "type": "object",
      "properties": {
        "closedTimestamp": {
          "type": "string",
          "description": "Timestamp when the alert was closed."
        },
        "createdTimestamp": {
          "type": "string",
          "description": "Timestamp when the alert was created."
        },
        "description": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "type": "object",
                "additionalProperties": {
                  "type": "string"
                }
              }
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Description of the alert."
        },
        "faultId": {
          "type": "string",
          "description": "Gets or sets the fault ID of the alert."
        },
        "alertId": {
          "type": "string",
          "description": "Gets or sets the ID of the alert."
        },
        "faultTypeId": {
          "type": "string",
          "description": "Gets or sets the fault type ID of the alert."
        },
        "lastUpdatedTimestamp": {
          "type": "string",
          "description": "Timestamp when the alert was last updated."
        },
        "alertProperties": {
          "oneOf": [
            {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Properties of the alert."
        },
        "remediation": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "type": "object",
                "additionalProperties": {
                  "type": "string"
                }
              }
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Gets or sets the admin friendly remediation instructions for the alert."
        },
        "resourceRegistrationId": {
          "type": "string",
          "description": "Gets or sets the registration ID of the resource associated with the alert. If the alert is not associated with a resource, the resource registration ID is null."
        },
        "resourceProviderRegistrationId": {
          "type": "string",
          "description": "Gets or sets the registration ID of the service the alert belongs to."
        },
        "severity": {
          "type": "string",
          "description": "Severity of the alert."
        },
        "state": {
          "type": "string",
          "description": "State of the alert."
        },
        "title": {
          "type": "string",
          "description": "Gets or sets the Resource ID for the impacted item."
        },
        "impactedResourceId": {
          "type": "string",
          "description": "Gets or sets the Resource ID for the impacted item."
        },
        "impactedResourceDisplayName": {
          "type": "string",
          "description": "Display name for the impacted item."
        },
        "closedByUserAlias": {
          "type": "string",
          "description": "User alias who closed the alert."
        },
        "hasValidRemediationAction": {
          "oneOf": [
            {
              "type": "boolean"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Indicates if the alert can be remediated."
        }
      },
      "description": "Contains alert data."
    }
  }
}
