{
  "id": "https://schema.management.azure.com/schemas/2016-01-01/Microsoft.AzureBridge.Admin.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Microsoft.AzureBridge.Admin",
  "description": "Microsoft AzureBridge Admin Resource Types",
  "resourceDefinitions": {
    "activations": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Name of the activation."
        },
        "type": {
          "type": "string",
          "enum": [
            "Microsoft.AzureBridge.Admin/activations"
          ]
        },
        "apiVersion": {
          "type": "string",
          "enum": [
            "2016-01-01"
          ]
        },
        "displayName": {
          "type": "string",
          "description": "Name displayed for the product."
        },
        "azureRegistrationResourceIdentifier": {
          "type": "string",
          "description": "Azure registration resource identifier."
        },
        "provisioningState": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Stopped",
                "Starting",
                "Running",
                "Stopping",
                "Succeeded",
                "Downloading"
              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Provisioning state of the resource."
        },
        "expiration": {
          "type": "string",
          "description": "The activation expiration."
        },
        "marketplaceSyndicationEnabled": {
          "oneOf": [
            {
              "type": "boolean"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Value indicating whether the marketplace syndication feature is enabled."
        },
        "usageReportingEnabled": {
          "oneOf": [
            {
              "type": "boolean"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Value indicating whether the usage reporting feature is enabled."
        }
      },
      "required": [
        "name",
        "type",
        "apiVersion"
      ],
      "description": "Microsoft.AzureBridge.Admin/activations"
    }
  }
}
