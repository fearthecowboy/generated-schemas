{
  "id": "https://schema.management.azure.com/schemas/2018-02-09/Microsoft.Compute.Admin.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Microsoft.Compute.Admin",
  "description": "Microsoft Compute Admin Resource Types",
  "resourceDefinitions": {
    "locations_quotas": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Name of the quota."
        },
        "type": {
          "type": "string",
          "enum": [
            "Microsoft.Compute.Admin/locations/quotas"
          ]
        },
        "apiVersion": {
          "type": "string",
          "enum": [
            "2018-02-09"
          ]
        },
        "location": {
          "type": "string",
          "description": "Location of the resource."
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/QuotaProperties"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Properties for a Compute Quota"
        }
      },
      "required": [
        "name",
        "type",
        "apiVersion",
        "properties"
      ],
      "description": "Microsoft.Compute.Admin/locations/quotas"
    }
  },
  "definitions": {
    "QuotaProperties": {
      "type": "object",
      "properties": {
        "availabilitySetCount": {
          "oneOf": [
            {
              "type": "integer",
              "minimum": 0
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Maximum number of availability sets allowed."
        },
        "coresLimit": {
          "oneOf": [
            {
              "type": "integer",
              "minimum": 0
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Maximum number of cores allowed."
        },
        "virtualMachineCount": {
          "oneOf": [
            {
              "type": "integer",
              "minimum": 0
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Maximum number of virtual machines allowed."
        },
        "vmScaleSetCount": {
          "oneOf": [
            {
              "type": "integer",
              "minimum": 0
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Maximum number of scale sets allowed."
        },
        "maxAllocationStandardManagedDisksAndSnapshots": {
          "oneOf": [
            {
              "type": "integer",
              "minimum": 0
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Maximum number of managed disks and snapshots of type standard allowed."
        },
        "maxAllocationPremiumManagedDisksAndSnapshots": {
          "oneOf": [
            {
              "type": "integer",
              "minimum": 0
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Maximum number of managed disks and snapshots of type premium allowed."
        }
      },
      "description": "Properties for a Compute Quota"
    }
  }
}
