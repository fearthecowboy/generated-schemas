                "AzureIaasVM",
                "MAB",
                "DPM",
                "AzureBackupServer",
                "AzureSql",
                "AzureStorage",
                "AzureWorkload",
                "DefaultBackup"
              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Type of backup management for the backed up item."
        },
        "workloadType": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Invalid",
                "VM",
                "FileFolder",
                "AzureSqlDb",
                "SQLDB",
                "Exchange",
                "Sharepoint",
                "VMwareVM",
                "SystemState",
                "Client",
