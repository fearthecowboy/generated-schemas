{
  "id": "https://schema.management.azure.com/schemas/2017-08-01-preview/Microsoft.Security.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Microsoft.Security",
  "description": "Microsoft Security Resource Types",
  "resourceDefinitions": {
    "pricings": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "name of the pricing configuration"
        },
        "type": {
          "type": "string",
          "enum": [
            "Microsoft.Security/pricings"
          ]
        },
        "apiVersion": {
          "type": "string",
          "enum": [
            "2017-08-01-preview"
          ]
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/PricingProperties"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Pricing data"
        }
      },
      "required": [
        "name",
        "type",
        "apiVersion",
        "properties"
      ],
      "description": "Microsoft.Security/pricings"
    }
  },
  "definitions": {
    "PricingProperties": {
      "type": "object",
      "properties": {
        "pricingTier": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Free",
                "Standard"
              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#/definitions/expression"
            }
          ],
          "description": "Pricing tier type."
        }
      },
      "required": [
        "pricingTier"
      ],
      "description": "Pricing data"
    }
  }
}
